{
  "name": "elm-hangman",
  "version": "0.0.1",
  "main": "index.js",
  "repository": "git@github.com:gregoryalbouy/elm-hangman",
  "author": "Gregory Albouy <gregoryalbouy75@gmail.com>",
  "license": "MIT",
  "scripts": {
    "dev": "elm-watch hot & static-server -p 9999 .",
    "build": "elm-watch make --optimize",
    "lint": "elm-format ./src --validate && elm-review",
    "lint:watch": "elm-review --watch",
    "test": "elm-test",
    "test:watch": "elm-test --watch",
    "checkall": "yarn lint && yarn test",
    "clear": "rm -rf ./elm-stuff/* ./build/*"
  },
  "devDependencies": {
    "elm": "^0.19.1-5",
    "elm-format": "^0.8.5",
    "elm-review": "^2.7.2",
    "elm-test": "^0.19.1-revision8",
    "elm-watch": "^1.0.0-beta.11",
    "static-server": "^2.2.1"
  }
}
